	number = {12},
	urldate = {2023-02-21},
	journal = {Physiological Genomics},
	author = {Thalacker-Mercer, Anna and Stec, Michael and Cui, Xiangqin and Cross, James and Windham, Samuel and Bamman, Marcas},
	month = jun,
